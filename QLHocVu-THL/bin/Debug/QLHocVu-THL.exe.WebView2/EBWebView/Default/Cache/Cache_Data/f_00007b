    <script async defer>

        window.addEventListener("load", function () {
            setTimeout(function () {
                var maxHeight = 0;
                $(".get-height").each(function () {
                    if ($(this).height() > maxHeight) {
                        maxHeight = $(this).height();
                    } else {
                        maxHeight = maxHeight;
                    }

                });
                $(".get-height").css("height", maxHeight + "px");

                $(function () {
                    let isMobile = window.matchMedia("only screen and (device-width: 768px)").matches;

                    if (isMobile) {
                        $(".set-height-news").css("height", maxHeight + "px");
                        $(".get-height").css("margin-bottom", "30px");
                    }
                });

                //if (/iPad/i.test(navigator.userAgent)) {

                //}

            }, 1000);
            setTimeout(function () {
                var maxHeight = 0;
                $(".get-height-event").each(function () {
                    if ($(this).height() > maxHeight) {
                        maxHeight = $(this).height();
                    } else {
                        maxHeight = maxHeight;
                    }

                });
                $(".get-height-event").css("height", maxHeight + "px");

            }, 1000);
        });

        $(document).ready(function () {

        });
    </script>

    
    
