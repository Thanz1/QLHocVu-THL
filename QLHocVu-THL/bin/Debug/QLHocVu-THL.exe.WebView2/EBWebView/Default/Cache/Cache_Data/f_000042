    </script>

    <script async defer>

        window.addEventListener("load", function () {
            setTimeout(function () {
                var maxHeight = 0;
                $(".get-height").each(function () {
                    if ($(this).height() > maxHeight) {
                        maxHeight = $(this).height();
                    } else {
                        maxHeight = maxHeight;
                    }

                });
                $(".get-height").css("height", maxHeight + "px");

                $(function () {
                    let isMobile = window.matchMedia("only screen and (device-width: 768px)").matches;

                    if (isMobile) {
                        $(".set-height-news").css("height", maxHeight + "px");
                        $(".get-height").css("margin-bottom", "30px");
                    }
                });

                //if (/iPad/i.test(navigator.userAgent)) {

                //}

            }, 1000);
            setTimeout(function () {
                var maxHeight = 0;
                $(".get-height-event").each(function () {
                    if ($(this).height() > maxHeight) {
                        maxHeight = $(this).height();
                    } else {
                        maxHeight = maxHeight;
                    }

                });
                $(".get-height-event").css("height", maxHeight + "px");

            }, 1000);
        });

        $(document).ready(function () {

        });
    </script>

    
    
    <script defer>
        function LoadGroup(e) { $.ajax({ type: "GET", url: '/Search/PartialGroupResult', contenType: "application/JSON; charset=utf-8", data: { key: e }, dataType: "html", beforeSend: function () { $("#groupresultcontent").html(null), $("#resultcontent").html(null), $(".search-loader").show(), $(".search-subloader").hide() }, complete: function () { $(".search-loader").hide() }, success: function (t) { if (!t.includes("Không tìm thấy kết quả tìm kiếm cho từ khóa")) { LoadResult(e, null) } $("#groupresultcontent").html(t)},error:function(){}})}function LoadResult(e,t){$.ajax({type:"GET",url:'/Search/PartialResult',contenType:"application/JSON; charset=utf-8",data:{key:e,idnhombd:t},dataType:"html",beforeSend:function(){$("#resultcontent").html(null),$(".search-loader").hide(),$(".search-subloader").show()},complete:function(){$(".search-subloader").hide()},success:function(e){$("#resultcontent").html(e)},error:function(){}})}function SelectGroup(e){var t=$("#txtSearch").val().trim();$(".list-group-item").removeClass("active"),LoadResult(t,e),$("#menu-item-"+e).addClass("active")}$("#txtSearchHome").on("input",function(){var e=$("#txtSearchHome").val();""!==e.trim()?($("#txtSearch").val(e),$("#txtSearchHome").val(null),$("#formSearch").hide(),$("#renderBody").hide(),$("#searchContent").show(),$("#txtSearch").focus()):($("#renderBody").show(),$("#searchContent").hide(),$("#idEnglist").show())}),$("#txtSearchHomeMobile").on("input",function(){var e=$("#txtSearchHomeMobile").val();""!==e.trim()?($("#txtSearch").val(e),$("#txtSearchHomeMobile").val(null),$("#formSearch").hide(),$("#renderBody").hide(),$("#searchContent").show(),$("#txtSearch").focus()):($("#renderBody").show(),$("#searchContent").hide(),$("#idEnglist").show())}),$("#txtSearch").on("input",function(){delay(function(){var e=$("#txtSearch").val();""===e.trim()?($("#txtSearch").val(""),LoadGroup(null)):e.trim().length>1?LoadGroup(e.trim()):LoadGroup(null)},2e3)}),$("#btncancel").click(function(){$("#groupresultcontent").html(null),$("#resultcontent").html(null),$("#renderBody").show(),$("#searchContent").hide(),$("#formSearch").show()});var SearchnoData=!1,SearchCount=0,SearchinProgress=!1;function SearchGetMore(e){var t=$("#txtSearch").val(),o="#grp"+e,r=".item"+e,n="#listCount"+e,a="#btnXemthem"+e,c="#loading"+e;SearchCount=$(r).length,SearchinProgress||SearchnoData||$.ajax({type:"GET",url:'/Search/PartialLoad',contenType:"application/JSON; charset=utf-8",data:{key:t,idnhombd:e,start:SearchCount},dataType:"html",beforeSend:function(){SearchinProgress=!0,$(a).hide(),$(c).show()},complete:function(){SearchinProgress=!1,$(c).hide()},success:function(e){$(o).append(e),(SearchCount=$(r).length)<$(n).text()&&$(a).show()},error:function(){noData=!0}})}$(".container-2 #txtSearchHome").hover(function(){$("#txtSearchHome").focus()}),$(".container-2 span").hover(function(){$("#txtSearchHome").focus()}),$(".box .container-2").mouseout(function(){$("#txtSearchHome").focus(),""===$("#txtSearchHome").val().trim()&&$("#txtSearchHome").val(null),$("#txtSearchHome").blur()});
